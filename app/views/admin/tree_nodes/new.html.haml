- title t('.title', :type => resource.class.name.capitalize)

- content_for :submenu do
	= render 'admin/shared/menu/tree_templates'
	
= semantic_form_for resource, :as => :tree_node, :url => collection_url do |form|
	= render 'form', :f => form
	
	= render 'admin/shared/form_actions', :back_url => collection_url, :button_label => :create